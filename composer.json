{
	"authors": [
		{
			"email": "vadimakeb@gmail.com",
			"name": "Vadim Babadzhanyan",
			"role": "Lead"
		}
	],
	"autoload": {
		"classmap": [
			"src/lib/"
		],
		"files": [
			"src/include/placeholder.php",
			"src/include/constant.php",
			"src/include/common.php",
			"src/init.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"archive": {
		"exclude": ["example/", "vendor/", "docker/", "src/css/", "src/js/", "tests/"]
	},
	"archive-dev": {
		"exclude": ["example/", "vendor/", "docker/", "src/css/", "src/js/"]
	},
	"description": "Framework",
	"license": "Apache-2.0",
	"name": "akeb/framework",
	"support": {
		"email": "vadimakeb@gmail.com",
		"issues": "https://github.com/AKEB/Framework-AKEB-framework/issues",
		"wiki": "https://github.com/AKEB/Framework-AKEB-framework/wiki",
		"source": "https://github.com/AKEB/Framework-AKEB-framework"
	},
	"type": "library",
	"require": {
		"php": ">=8.2",
		"akeb/curlget": "@stable",
		"akeb/profiler": "@stable",
		"akeb/cache": "@stable",
		"phpmailer/phpmailer": "@stable",
		"jumbojett/openid-connect-php": "@stable",
		"chillerlan/php-qrcode": "@stable",
		"telegram-bot/api": "@stable"
	},
	"require-dev": {
		"phpunit/phpunit": "@stable",
		"akeb/minify": "@stable",
		"bissolli/php-css-js-minifier": "^1.1"
	}
}
